<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jackzhao.www.bottomactionbar">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.Home" />
        <activity android:name=".activities.History" />
        <activity android:name=".activities.Favorite" />
        <activity android:name=".activities.User" />
        <activity android:name=".activities.Search" />
        <activity android:name=".components.listview_item_company"></activity>
    </application>

</manifest>